public class Main{
    public static void main(String[] args){
       int[] arr ={11,2,3,2,4,3,4};
        System.out.print(ans(arr));

    }
    static int ans(int[] arr){
        int unique = 0;
        for(int n : arr){
            unique = unique ^ n;
            
        }
        return unique;
    }}
    
