public class Main {
    public static void main(String[] args) {
      int n = 4;
        System.out.println(Integer.toBinaryString(n));
        System.out.println(setbit(n));

    }

    static int setbit(int n){
        int count = 0;
        while(n > 0){
            count +=1;
            n = n & (n-1);
        }
        return count;
    }}
